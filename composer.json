{
    "name": "floravel/human-comparator",
    "description": "A package that returns some interesting data about the specific human.",
    "keywords": [
        "floravel",
        "human-comparator"
    ],
    "homepage": "https://github.com/floravel/human-comparator",
    "license": "MIT",
    "authors": [
        {
            "name": "Florian He√ü",
            "email": "flo-hess1@web.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.17",
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Floravel\\HumanComparator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Floravel\\HumanComparator\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
